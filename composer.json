{
	"name": "fromholdio/silverstripe-globalanchors",
	"type": "silverstripe-vendormodule",
	"description": "A SilverStripe module that allows developers to define a static list of global html anchors - #nav, #content, etc - allowing them to be used for constructing links and etc by CMS users.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "cms", "anchor", "links"],
	"homepage": "https://github.com/fromholdio/silverstripe-systemlinks",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-systemlinks/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
	"require": {
        "silverstripe/framework": "~4.0 || ~5.0 || ^6.0"
	},
	"autoload": {
		"psr-4": {
			"Fromholdio\\GlobalAnchors\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
